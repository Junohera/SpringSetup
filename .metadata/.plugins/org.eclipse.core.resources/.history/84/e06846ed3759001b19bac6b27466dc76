package fint;

public class MainClass {
	public static void main(String[] args) {
		Calculator c;
		CalMul cm = new CalMul();
		c = cm;
		int result = c.cal(10, 30);

		
		CalDiv cd = new CalDiv();
		c = cd;
		result = c.cal(30, 4);
		
		CalSum cs = new CalSum();
		c = cd;
		result = c.cal(30, 4);
		
		CalSub cb = new CalSub();
		c = cd;
		result = c.cal(30, 4);
		
	}
}
